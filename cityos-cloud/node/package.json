    "handlebars": "^4.7.7",
    "hbs": "^4.2.0",
    "ioredis": "^4.28.3",
    "kafkajs": "^1.15.0",
    "luxon": "^2.3.0",
    "mjml": "^4.11.0",
    "mongodb": "^4.2.2",
    "mongoose": "^6.3.2",
    "mongoose-autopopulate": "^0.16.1",
    "mqtt": "^4.3.7",
    "multer": "^1.4.5-lts.1",
    "nestjs-i18n": "^9.0.10",
    "nodemailer": "^6.6.2",
    "oauth2-server": "^4.0.0-dev.3",
    "passport": "^0.4.1",
