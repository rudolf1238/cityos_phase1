{
  "name": "city-os-cloud",
  "version": "2.4.6",
  "description": "The server of the CityOS platform collaborated with the CHT.",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist && npm run schema",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "npm run schema",
    "start": "nest start",
    "start:dev": "npm start --watch",
    "start:hmr": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "npm start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "nest build -p tsconfig.test.json && cp ./test/jest-e2e.json ./dist-test/test/",
    "test:e2e": "jest --config ./dist-test/test/jest-e2e.json --forceExit",
    "schema": "ts-node tools/generate-typings"
  },
  "dependencies": {
    "@casl/ability": "^5.2.2",
    "@elastic/elasticsearch": "7.13.0",
    "@line/bot-sdk": "^7.5.0",
